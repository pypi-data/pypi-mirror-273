{
  "scheming_presets_version": 1,
  "about": "IDS custom scheming field presets",
  "about_url": "http://github.com/ckan/ckanext-scheming#preset",
  "presets": [
    {
      "preset_name": "package_slug",
      "values": {
        "validators": "not_empty unicode name_validator package_name_validator",
        "form_snippet": "slug.html"
      }
    },
    {
      "preset_name": "access_url",
      "values": {
        "validators": "url_validator",
        "form_snippet": "text.html"
      }
    },
    {
      "preset_name": "trusts_access_url",
      "values": {
        "validators": "trusts_url_validator",
        "form_snippet": "text.html"
      }
    },
    {
      "preset_name": "disabled",
      "values": {
        "validators": "ignore_missing",
        "form_snippet": "text.html",
        "display_snippet": null,
        "form_placeholder": "...this will be filled automatically.",
        "form_attrs": {
          "readonly": ""
        }
      }
    }
  ]
}
