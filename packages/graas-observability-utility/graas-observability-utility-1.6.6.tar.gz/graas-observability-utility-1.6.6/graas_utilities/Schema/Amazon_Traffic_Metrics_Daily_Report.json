{
  "type": "object",
  "title": "Amazon Traffic Metrics Daily Report",
  "description": "Daily report providing key metrics related to traffic and sales on Amazon",
  "properties": {
    "Date": {
      "type": "string",
      "description": "Date of the report in the format 'MM-DD-YYYY'",
      "format": "dd-MM-yyyy",
      "pattern": "[0-9]{2}-[0-9]{2}-[1-2][0-9]{3}"
    },
    "Ordered_Product_Sales": {
      "type": "object",
      "$ref": "../Schema/amount_currency_code_object.json",
      "description": "Sales information for ordered products"
    },
    "Units_Ordered": {
      "type": "integer",
      "description": "Total number of units ordered"
    },
    "Total_Order_Items": {
      "type": "integer",
      "description": "Total number of order items"
    },
    "Page_Views_Total": {
      "type": "integer",
      "description": "Total number of page views"
    },
    "Sessions_Total": {
      "type": "integer",
      "description": "Total number of sessions"
    },
    "Featured_Offer_Buy_Box_Percentage": {
      "type": "number",
      "description": "Percentage of time the featured offer is in the buy box"
    },
    "Unit_Session_Percentage": {
      "type": "number",
      "description": "Percentage of units ordered per session"
    },
    "Average_Offer_Count": {
      "type": "integer",
      "description": "Average number of offers available for the product"
    },
    "Average_Parent_Items": {
      "type": "integer",
      "description": "Average number of parent items"
    }
  },
  "required": [
    "Date"
  ]
}